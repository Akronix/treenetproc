citHeader("To cite treenetproc in publications use:")

citEntry(
  entry       = "Manual",
  title       = "treenetproc: towards a standardized processing of stem radius data",
  author      = c(person("Simon", "Knüsel"), person("Matthias", "Haeni"),
  person("Micah", "Wilhelm"), person("Richard L.", "Peters"), person("Roman", "Zweifel")),
  year        = "2020",
  url         = "https://github.com/treenet/treenetproc",
  textVersion = "Knüsel S., Haeni M., Wilhelm M., Peters R.L., Zweifel R. 2020. treenetproc: towards a standardized processing of stem radius data. In prep."
)

citEntry(
  entry       = "Manual",
  title       = "treenetproc - Clean, process and visualise dendrometer data",
  author      = c(person("Matthias", "Haeni"), person("Simon", "Knüsel"),
  person("Micah", "Wilhelm"), person("Richard L.", "Peters"), person("Roman", "Zweifel")),
  year        = "2020",
  note        = "Github repository",
  url         = "https://github.com/treenet/treenetproc",
  textVersion = "Haeni M., Knüsel S., Wilhelm M., Peters R.L., Zweifel R. 2020. treenetproc - Clean, process and visualise dendrometer data. R package version 0.1.4. Github repository: https://github.com/treenet/treenetproc"
)

